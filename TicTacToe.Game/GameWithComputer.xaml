<UserControl x:Class="TicTacToe.Game.GameWithComputer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TicTacToe.Game"
             mc:Ignorable="d" 
             d:DesignHeight="350" d:DesignWidth="525">
    <Canvas x:Name="grid" Background="Transparent">
        <Button x:Name="button" Content="Back" HorizontalAlignment="Right" VerticalAlignment="Top" Width="75" Click="button_Click" Canvas.Left="440" Canvas.Top="10" Margin="-15,0,0,0"/>

    </Canvas>
</UserControl>
